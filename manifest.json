{
  "manifest_version": 2,

  "name": "Trello Viewer",
  "version": "0.1",

  "default_locale": "jp",
  "author": "kyoh86",
  "homepage_url": "https://github.com/kyoh86/trelloviewer",
  "description": "This extension show cards/lists/boards on trello clearly.",

  "permissions": [
    "https://trello.com/",
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "html/popup.html",
  },
  "options_page": "html/options.html",
}
